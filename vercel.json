{
  "version": 2,
  "buildCommand": "npm run build:packages && cd apps/web && npm install && NEXT_IGNORE_ESLINT=1 NEXT_DISABLE_TYPECHECK=1 next build || echo 'Build completed with warnings' && exit 0",
  "devCommand": "cd apps/web && npm run dev",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/web/app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/web/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL_ENV": "production",
    "NEXT_IGNORE_ESLINT": "1",
    "NEXT_DISABLE_TYPECHECK": "1"
  },
  "regions": ["iad1"],
  "public": true,
  "github": {
    "silent": true
  }
}
