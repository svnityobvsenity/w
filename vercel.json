{
  "version": 2,
  "buildCommand": "cd apps/web && npm install && next build",
  "devCommand": "cd apps/web && npm run dev",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/web/app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/web/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "public": true,
  "github": {
    "silent": true
  }
}
